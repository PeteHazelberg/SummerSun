@model SummerSunMVC.Models.PointViewModel

@{
    ViewBag.Title = "PointDetailView";
}
<br />
<p> Maybe a D3.JS chart ? Let's start with Highchart. Wait a couple of seconds...</p>

<div id="chartContainer" style="width:100%; height:400px;"></div>
<input type="hidden" value="@ViewBag.AccessToken" id="AccessToken" />
<input type="hidden" value="@ViewBag.pointUrl" id="pointUrl" />

@section scripts {

<script src="http://code.highcharts.com/highcharts.js"></script>
@Scripts.Render("~/Scripts/TimeSeriesHighChart.js")

<script>
    InitChart();
</script>

}